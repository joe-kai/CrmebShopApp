<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTitleBar">
        <attr name="appTitle" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <attr name="leftIcon" format="reference" />
        <attr name="leftTitle" format="string" />
        <attr name="leftTitleColor" format="color" />
        <attr name="leftTitleSize" format="dimension" />
        <attr name="leftHide" format="boolean" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightTitle" format="string" />
        <attr name="rightTitleSize" format="dimension" />
        <attr name="rightTitleColor" format="color" />
        <attr name="rightHide" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TypefaceTextView">
        <attr name="typeface">
            <enum name="fzlLTypeface" value="1" />
            <enum name="fzdb1Typeface" value="2" />
            <enum name="futuraTypeface" value="3" />
            <enum name="dinTypeface" value="4" />
            <enum name="lobsterTypeface" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NoStatusFooter">
        <attr name="srlPrimaryColor" />
        <attr name="srlAccentColor" />
        <attr name="srlTextSizeTitle" />
        <attr name="srlTextTitleTypeface">
            <enum name="fzlLTypeface" value="1" />
            <enum name="fzdb1Typeface" value="2" />
            <enum name="futuraTypeface" value="3" />
            <enum name="dinTypeface" value="4" />
            <enum name="lobsterTypeface" value="5" />
        </attr>
        <attr name="srlTextNothing" />
    </declare-styleable>
</resources>